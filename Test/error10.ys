		.pos 0x0
		irmovq stack, %rsp
		call main
		halt
main:		
		irmovq n, %rbx 
		irmovq $-1, %rcx 
loop: 	mrmovq (%rbx), %rdx
		addq %rcx, %rdx 
		rmmovq %rdx, (%rbx)
		jne loop 
		ret
		.align 8
a:		.quad	0x0123456789abcdef
b:		.quad	0xfedcba9876543210
		.pos 0x1000
n: 		.quad 8 
		.pos 0x2000
